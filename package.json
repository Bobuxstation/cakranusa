{
  "name": "cakranusa",
  "version": "1.0.0",
  "description": "City planning and bureaucracy simulation game",
  "homepage": "https://github.com/Bobuxstation/cakranusa",
  "main": "main.js",
  "scripts": {
    "test": "echo \"No test specified\"",
    "start": "electron .",
    "dist": "electron-builder",
    "distCI": "electron-builder --win --linux --mac"
  },
  "repository": "https://github.com/Bobuxstation/cakranusa",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "author": "Perintis Interactive",
  "license": "MIT",
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "@faker-js/faker": "^10.2.0",
    "electron-updater": "^5.3.0"
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^20.44.4"
  },
  "build": {
    "appId": "com.perintisinteractive.Cakranusa",
    "productName": "Cakranusa",
    "win": {
      "verifyUpdateCodeSignature": false,
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publisherName": "Perintis Interactive"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage",
        "snap",
        "rpm"
      ],
      "maintainer": "Perintis Interactive",
      "category": "Game"
    },
    "mac": {
      "target": [
        "zip",
        "tar.gz"
      ],
      "category": "public.app-category.games"
    }
  }
}